<h1>Listing photos</h1>

<table>
  <tr>
    <th>User</th>
    <th>Event</th>
    <th>Caption</th>
    <th>Lat</th>
    <th>Lon</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.user %></td>
    <td><%= photo.event %></td>
    <td><%= photo.caption %></td>
    <td><%= photo.lat %></td>
    <td><%= photo.lon %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_event_photo_path %>
